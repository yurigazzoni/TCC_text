    \newcommand{\membrodabanca}[3][]{%
        \def\@opnull{}%
        \def\@op{#1}%
        \ifx\@opnull\@op
            \ifnum\value{@nmembrosbanca}>0%
                \def\@tipomembro{Membro(a)}%
            \else%
                \def\@tipomembro{\imprimirorientadorRotulolimpo/Presidente}%
            \fi%
        \else%
            \def\@tipomembro{#1}%
        \fi%

        \ifnum\value{@flfolhadeaprovacao}>0%

            \vfill%
            \begin{minipage}{10cm}
                \begin{center}
                    \rule{0.75\textwidth}{0.3pt}

                    #2

                    \@tipomembro%#1

                    \abntex@ifnotempty{#3}{#3}%\ifnum\value{@nmembrosbanca}>0{#3}\fi%
                    \addtocounter{@nmembrosbanca}{1}%
                \end{center}%

            \end{minipage}\par\fi}


