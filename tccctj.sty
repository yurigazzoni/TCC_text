    \newcommand{\palavrachave}[2][\@titulopalavraschave]{%
        \ifnum\value{@flresumo}>0%
            \addtocounter{@nkeyword}{1}%
            \ifnum\value{@nkeyword}<2
                \savebox{\@palavraschaves}{\textbf{#1: }\@sublimpaponto(#2.@null).}%
                \renewcommand{\@dicapalavraschave}{\@excmdred{palavrachave}\@exargs{\@bluearg{segunda palavra, ou termo}}}%
            \else%
                \savebox{\@palavraschaves}{\raggedright\unhbox\@palavraschaves\ \@sublimpaponto(#2.@null).}%
                \ifnum\value{@nkeyword}<3
                    \renewcommand{\@dicapalavraschave}{\@excmdred{palavrachave}\@exargs{\@bluearg{terceira palavra, ou termo}}}%
                \else%
                    \ifnum\value{@nkeyword}<4
                        \renewcommand{\@dicapalavraschave}{\@excmdred{palavrachave}\@exargs{\@bluearg{quarta palavra-chave \textbf{opcional}}}}%
                    \else%
                        \ifnum\value{@nkeyword}<5
                            \renewcommand{\@dicapalavraschave}{\@excmdred{palavrachave}\@exargs{\@bluearg{última palavra-chava \textbf{opcional}}}}%
                        \else%
                            \renewcommand{\@dicapalavraschave}{}%
                            \ifnum\value{@nkeyword}>5
                                \renewcommand{\@dicapalavraschave}{\textcolor{red}{O número máximo de palavras-chaves deveria ser cinco (5)!}}%
                            \fi%
                        \fi%
                    \fi%
                \fi%
            \fi%
        \fi}

