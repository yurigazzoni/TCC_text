    \def\@subexargs(#1,#2){%
        \def\@close{:close:}%
        \def\@secondarg{#2}%
        \textsf{\{#1\}}%
        \ifx\@secondarg\@close\else{\@subexargs(#2)}\fi}

    \def\@sublastname(#1 #2){%
        \def\@close{:close:}%
        \def\@secondarg{#2}%
        \ifx\@secondarg\@close{#1}\else{\@sublastname(#2)}\fi}
    %     \ifx\@secondarg\@close{#1}\else{B}\fi}

    \def\@inicial(#1#2){#1}

    \def\@filtroDA(#1){%
        \def\@arg{#1}%
        \def\@da{DA}%
        \ifx\@arg\@da{}\else{%
                \def\@de{DE}%
                \ifx\@arg\@de{}\else{%
                        \def\@do{DO}%
                        \ifx\@arg\@do{}\else{\@inicial(#1).~}\fi%
                    }\fi}\fi}

    \def\@filtroda(#1){%
        \def\@arg{#1}%
        \def\@da{da}%
        \ifx\@arg\@da{}\else{%
                \def\@de{de}%
                \ifx\@arg\@de{}\else{%
                        \def\@do{do}%
                        \ifx\@arg\@do{}\else{\@filtroDA(#1)}\fi%
                    }\fi}\fi}

    \def\@subnomeabrev(#1 #2){%
        \def\@close{:close:}%
        \def\@secondarg{#2}%
        \ifx\@secondarg\@close{}\else{\@filtroda(#1)\@subnomeabrev(#2)}\fi}

    \def\@filtroDAsigla(#1){%
        \def\@arg{#1}%
        \def\@da{DA}%
        \ifx\@arg\@da{}\else{%
                \def\@de{DE}%
                \ifx\@arg\@de{}\else{%
                        \def\@do{DO}%
                        \ifx\@arg\@do{}\else{\@inicial(#1)}\fi%
                    }\fi}\fi}

    \def\@filtrodasigla(#1){%
        \def\@arg{#1}%
        \def\@da{da}%
        \ifx\@arg\@da{}\else{%
                \def\@de{de}%
                \ifx\@arg\@de{}\else{%
                        \def\@do{do}%
                        \ifx\@arg\@do{}\else{\@filtroDAsigla(#1)}\fi%
                    }\fi}\fi}

    \def\@subsigla(#1 #2){%
        \uppercase{\@filtrodasigla(#1)}%
        \def\@close{:close:}%
        \def\@secondarg{#2}%
        \ifx\@secondarg\@close{}\else{\@subsigla(#2)}\fi}

    \def\@sublimpaponto(#1.#2){#1}

    \def\@sublimpadoispontos(#1:#2){#1}




